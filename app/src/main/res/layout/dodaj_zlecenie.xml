<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/l18"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activities.DodajZlecenie">

    <ImageView
        android:id="@+id/img18LeftArrow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:padding="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/img18RightArrow"
        app:layout_constraintEnd_toStartOf="@+id/img18DotOne"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/img18RightArrow"
        app:srcCompat="@drawable/left" />

    <ImageView
        android:id="@+id/img18DotOne"
        android:layout_width="12dp"
        android:layout_height="12dp"
        android:cropToPadding="false"
        app:layout_constraintBottom_toBottomOf="@+id/img18RightArrow"
        app:layout_constraintEnd_toStartOf="@+id/img18DotTwo"
        app:layout_constraintStart_toEndOf="@+id/img18LeftArrow"
        app:layout_constraintTop_toTopOf="@+id/img18RightArrow"
        app:srcCompat="@drawable/dot" />

    <ImageView
        android:id="@+id/img18DotTwo"
        android:layout_width="12dp"
        android:layout_height="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/img18RightArrow"
        app:layout_constraintEnd_toStartOf="@+id/img18DotThree"
        app:layout_constraintStart_toEndOf="@+id/img18DotOne"
        app:layout_constraintTop_toTopOf="@+id/img18RightArrow"
        app:srcCompat="@drawable/dot" />

    <ImageView
        android:id="@+id/img18DotThree"
        android:layout_width="12dp"
        android:layout_height="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/img18RightArrow"
        app:layout_constraintEnd_toStartOf="@+id/img18DotFour"
        app:layout_constraintStart_toEndOf="@+id/img18DotTwo"
        app:layout_constraintTop_toTopOf="@+id/img18RightArrow"
        app:srcCompat="@drawable/dot" />

    <ImageView
        android:id="@+id/img18DotFour"
        android:layout_width="12dp"
        android:layout_height="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/img18RightArrow"
        app:layout_constraintEnd_toStartOf="@+id/img18DotFive"
        app:layout_constraintStart_toEndOf="@+id/img18DotThree"
        app:layout_constraintTop_toTopOf="@+id/img18RightArrow"
        app:srcCompat="@drawable/dot" />

    <ImageView
        android:id="@+id/img18DotFive"
        android:layout_width="12dp"
        android:layout_height="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/img18RightArrow"
        app:layout_constraintEnd_toStartOf="@+id/img18RightArrow"
        app:layout_constraintStart_toEndOf="@+id/img18DotFour"
        app:layout_constraintTop_toTopOf="@+id/img18RightArrow"
        app:srcCompat="@drawable/dot" />

    <ImageView
        android:id="@+id/img18RightArrow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="40dp"
        android:padding="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/img18DotFive"
        app:srcCompat="@drawable/right" />

    <LinearLayout
        android:id="@+id/l18ForFragment"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/img18LeftArrow"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"></LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>